<?xml version="1.0"?>

<!--F-100D AC loadmeter-->

<PropertyList>

<path>loadmeter1.ac</path>

<!--***lighting***-->

<animation>
	<type>material</type>
	<object-name>indicator</object-name>
	<emission>
		<red>1.0</red>
		<green>0.25</green>
		<blue>0.25</blue>
		<factor-prop>controls/lighting/panel-norm</factor-prop>
	</emission> 
</animation> 

	<!-- *** LoD *** -->
	<animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>5</max-m>
	</animation>

</PropertyList>
